        "path": "Extension.extension.url",
        "fixedUri": "aerztlich"
      },
      {
        "id": "Extension.extension:aerztlich.value[x]",
        "path": "Extension.extension.value[x]",
        "slicing": {
          "discriminator": [
            {
              "type": "type",
              "path": "$this"
            }
          ],
          "ordered": false,
          "rules": "open"
        },
        "min": 1
      },
      {
        "id": "Extension.extension:aerztlich.valueCode",
        "path": "Extension.extension.valueCode",
        "min": 1,
        "max": "1",
        "type": [
          {
            "code": "code"
          }
        ],
        "constraint": [
          {
            "key": "gkv-1",
            "severity": "error",
            "human": "Es sind nur die Werte \"0\" (ärztlicher Selektivvertrag liegt nicht vor), \"1\" (ärztlicher Selektivvertrag liegt vor) oder \"9\" (ärztliches Selektivvertragskennzeichen wird nicht genutzt) erlaubt.",
            "expression": "matches('[019]')",
            "source": "https://gematik.de/fhir/eeb/StructureDefinition/Selektivvertraege"
          }
        ]
      },
      {
        "id": "Extension.extension:zahnaerztlich",
        "path": "Extension.extension",
